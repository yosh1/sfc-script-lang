<h2>courses</h2>
<table>
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @course.each do |c| %>
      <tr>
        <td><%= c.id %></td>
        <td><%= c.name %></td>
        <td><a href="<%= c.id %>">jump</a></td>
      </tr>
    <% end %>
  </tbody>
</table>